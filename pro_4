using System;  
  public class FactorialExample  
   {  
     public static void Main(string[] args)  
      {  
       int k,fact=1,number;      
       Console.Write("Enter any Number: ");      
       number= int.Parse(Console.ReadLine());     
       for(k=1;k<=number;k++){      
        fact=fact*k;      
       }      
       Console.Write("Factorial of " +number+" is: "+fact);    
     }  
  }  
